# Session Secret - MUST be set to a strong random value in production
# Generate with: openssl rand -base64 32
SESSION_SECRET=

# Database URL - Required for PostgreSQL session storage in production
# Format: postgresql://user:password@host:port/database
DATABASE_URL=

# SendGrid API Key for email notifications
SENDGRID_API_KEY=

# SendGrid verified sender email
SENDGRID_FROM_EMAIL=

# Node Environment
NODE_ENV=development

# Server Port
PORT=5000
